[alias]
    lol = log --graph --decorate --pretty=format:'%C(yellow)%h %C(green)(%ar) %C(cyan)%an %C(bold white)|%C(reset) %C(white)%s%C(reset)%C(auto)%d%C(reset)' --abbrev-commit
	lola = log --graph --decorate --pretty=format:'%C(yellow)%h %C(green)(%ar) %C(cyan)%an %C(bold white)|%C(reset) %C(white)%s%C(reset)%C(auto)%d%C(reset)' --abbrev-commit --all
	lolr = log --graph --decorate --pretty=format:'%C(yellow)%h %C(green)(%ar) %C(cyan)%an %C(bold white)|%C(reset) %C(white)%s%C(reset)%C(auto)%d%C(reset)' --abbrev-commit --exclude=refs/heads/release --exclude=refs/remotes/origin/release --exclude=refs/tags/release_* --all
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    ll = log -U -n 1
    co = checkout
    ci = commit -v
    ri = rebase -i
    chp = cherry-pick -ex
    st = status
    br = branch
    re = rebase
